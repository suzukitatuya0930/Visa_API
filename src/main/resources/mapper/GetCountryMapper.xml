<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.GetCountryMapper">

    <select id="getCountry" resultType="com.example.demo.model.GetCountryModel" >
   		SELECT  country_id,country_name FROM country_master WHERE delete_flag = 0
   		order by country_name ASC;
    </select>

</mapper>

